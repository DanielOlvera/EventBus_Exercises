<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.eventbus_example">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".withoutEventBus.WithoutEventBus"
            android:label="NO EventBus"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".withEventBus.WithEventBus"
            android:label="GREENROBOT EventBus"
            android:parentActivityName=".withEventBus.WithEventBus" />
        <activity
            android:name=".withOtto.WithOTTO"
            android:label="OTTO Event Bus"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".withRxJava.WithRxJava"
            android:label="RxJava Event Bus"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".withRxBus.WithRxBus"
            android:label="RxBus Event Bus"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>